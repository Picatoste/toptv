<script type="text/javascript">
	$(function(){ 
		App.start();
		
		//ui event listeners
		onChangeSortOrder = function()
		{
			App.sortBy($('#sortField').val(), $('#sortOrder').val());
		}
		
		//ui event listeners
		onTituloFilter=function()
		{
			App.filterBy($('#titulo').val());
		}
		
		
	});
</script>
